<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".logo_selector">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="149dp"
        android:text="Select a new Logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/teamid1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="47dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toTopOf="@+id/teamid4"
        app:layout_constraintEnd_toStartOf="@+id/teamid2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_01" />

    <ImageView
        android:id="@+id/teamid2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="2dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toStartOf="@+id/teamid3"
        app:layout_constraintStart_toEndOf="@+id/teamid1"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/ic_logo_02" />

    <ImageView
        android:id="@+id/teamid3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="48dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toTopOf="@+id/teamid6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_03" />

    <ImageView
        android:id="@+id/teamid4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="47dp"
        android:onClick="setTeamIcon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamid1"
        app:srcCompat="@drawable/ic_logo_04" />

    <ImageView
        android:id="@+id/teamid5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="303dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/teamid4"
        app:layout_constraintTop_toBottomOf="@+id/teamid2"
        app:srcCompat="@drawable/ic_logo_05" />

    <ImageView
        android:id="@+id/teamid6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="303dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/teamid5"
        app:layout_constraintTop_toBottomOf="@+id/teamid2"
        app:srcCompat="@drawable/ic_logo_00" />
</androidx.constraintlayout.widget.ConstraintLayout>